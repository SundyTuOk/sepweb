<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC     
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sf.login">
<!--User实体，USERS数据库表面名-->
    <class name="Users" table="USERS">
        <!--自增id-->
        <id name="USERS_ID">
            <generator class="native"/>
        </id>
        
        <property name="AREA_ID" 			column="AREA_ID"/>
        <property name="USERS_NUM" 			column="USERS_NUM"/>
        <property name="USERS_NAME" 		column="USERS_NAME"/>
        <property name="USERS_LOGINNAME" 	column="USERS_LOGINNAME"/>
        <property name="USERS_PASSWORD" 	column="USERS_PASSWORD"/>
        <property name="USERS_GENDER" 			column="USERS_GENDER"/>
        <property name="USERS_BIRTH" 			column="USERS_BIRTH"/>
        <property name="USERS_PHOTO" 			column="USERS_PHOTO"/>
        <property name="USERS_DEPARTMENT" 			column="USERS_DEPARTMENT"/>
        <property name="USERS_PHONE" 			column="USERS_PHONE"/>
        <property name="USERS_PHONE1" 			column="USERS_PHONE1"/>
        <property name="USERS_PHONE2" 			column="USERS_PHONE2"/>
        <property name="USERS_HOMEADDRESS" 			column="USERS_HOMEADDRESS"/>
        <property name="USERS_LASTTIME" 			column="USERS_LASTTIME"/>
        <property name="USERS_REGTIME" 			column="USERS_REGTIME"/>
        <property name="USERSTATE_ID" 			column="USERSTATE_ID"/>
        <property name="USERS_REMARK" 			column="USERS_REMARK"/>
        <property name="ONLINERE" 			column="ONLINERE"/>
        <property name="IP" 			column="IP"/>
        <property name="ISALARM" 			column="ISALARM"/>
        <property name="URLHOST" 			column="URLHOST"/>
    </class>

</hibernate-mapping>